{
    "name": "coffeephp/tarball",
    "description": "Provides classes for handling tarball compression.",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Danny Damsky",
            "email": "dannydamsky99@gmail.com"
        }
    ],
    "keywords": [
        "coffeephp",
        "coffee",
        "php",
        "compress",
        "compression",
        "compressor",
        "uncompress",
        "uncompression",
        "uncompressor",
        "decompress",
        "decompression",
        "decompressor",
        "method",
        "archive",
        "tar",
        "tarball"
    ],
    "version": "1.1.0",
    "time": "2021-04-23",
    "readme": "README.md",
    "require": {
        "php": ">=8.0",
        "ext-apcu": "*",
        "coffeephp/compression-method": "1.*"
    },
    "require-dev": {
        "coffeephp/component-registry": "1.*",
        "coffeephp/quality-tools": "1.*",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "CoffeePhp\\Tarball\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CoffeePhp\\Tarball\\Test\\Integration\\": "test/Integration/",
            "CoffeePhp\\Tarball\\Test\\Unit\\": "test/Unit/"
        }
    },
    "suggest": {
        "coffeephp/component-registry": "For integration of the component with the CoffeePHP framework."
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "apcu-autoloader": true
    },
    "scripts": {
        "coffeephp:exec:phpunit": "coffeephp_exec_phpunit --testsuite unit,integration",
        "coffeephp:exec:psalm": "coffeephp_exec_psalm",
        "coffeephp:exec:publish": "coffeephp_exec_publish"
    }
}
